{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "terms.html",
      "use": "@vercel/static"
    },
    {
      "src": "privacy.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/linked-role", "dest": "server.js" },
    { "src": "/callback", "dest": "server.js" },
    { "src": "/register-metadata", "dest": "server.js" },
    { "src": "/terms.html", "dest": "terms.html" },
    { "src": "/privacy.html", "dest": "privacy.html" },
    { "src": "/", "dest": "index.html" }
  ]
}
